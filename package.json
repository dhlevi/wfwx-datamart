{
  "name": "wfwx-datamart-api",
  "version": "1.0.0",
  "description": "Boilerplate",
  "main": "service/index.ts",
  "scripts": {
    "start": "ts-node lib/index.ts",
    "swagger": "tsoa spec",
    "copy-public": "copyfiles -u 1 ./lib/public/* build",
    "copy-config": "copyfiles ./config/* build",
    "copy-queries": "copyfiles -u 1 ./lib/query-configs/* build",
    "build": "npm run swagger && tsc && npm run copy-public && npm run copy-config && npm run copy-queries",
    "build-no-swagger": "tsc && npm run copy-public && npm run copy-config && npm run copy-queries",
    "prettier:check": "prettier --check service/**/*.{ts,tsx,js,jsx}",
    "prettier:write": "prettier --write service/**/*.{ts,tsx,js,jsx}"
  },
  "dependencies": {
    "@mw-experts/rsql": "2.2.1",
    "axios": "0.27.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "jsonwebtoken": "8.5.1",
    "multer": "1.4.5-lts.1",
    "mybatis-mapper": "0.6.8",
    "node-cron": "3.0.2",
    "pg": "8.8.0",
    "properties-reader": "2.2.0",
    "swagger-ui-express": "4.4.0",
    "tsoa": "4.1.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/helmet": "4.0.0",
    "@types/node-cron": "3.0.4",
    "@types/jsonwebtoken": "8.5.8",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.36",
    "@types/properties-reader": "2.1.1",
    "@types/swagger-ui-express": "4.1.3",
    "@types/uuid": "8.3.4",
    "@types/pg": "8.6.5",
    "copyfiles": "2.4.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0",
    "tslib": "2.4.0",
    "typescript": "4.8.4"
  },
  "license": "MIT"
}
